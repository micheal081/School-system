      <div class="sidebar">
        <div class="user-profile">
          <div class="display-avatar animated-avatar">
            <img class="profile-img img-lg rounded-circle" src="../assets/images/uploads/<% if(students.image == '') { %>dummy.jpg<% } else { %><%= students.image %><% } %>" alt="profile image">
          </div>
          <div class="info-wrapper">
            <p class="user-name"><%= students.name %></p>
            <p class="display-income"><%= students.email %></p>
          </div>
        </div>
        <ul class="navigation-menu">
          <li class="nav-category-divider">MAIN</li>
          <li>
            <a href="index?q=<%= userId %>">
              <span class="link-title">Dashboard</span>
              <i class="mdi mdi-gauge link-icon"></i>
            </a>
          </li>
          <li>
            <a href="#course" data-toggle="collapse" aria-expanded="false">
              <span class="link-title">Courses</span>
              <i class="mdi mdi-pen link-icon"></i>
            </a>
            <ul class="collapse navigation-submenu" id="course">
              <li>
                <a href="mycourse?q=<%= userId %>">My course</a>
              </li>
              <li>
                <a href="a-course?q=<%= userId %>">Available courses</a>
              </li>
            </ul>
          </li>
          <li class="nav-category-divider">PROFILE</li>
          <li>
            <a href="user-profile?q=<%= userId %>">
              <span class="link-title">My Profile</span>
              <i class="mdi mdi-account-circle link-icon"></i>
            </a>
          </li>
          <li class="nav-category-divider">SIGN OUT</li>
          <li>
            <a href="/logout">
              <span class="link-title">Log out</span>
              <i class="mdi mdi-logout link-icon"></i>
            </a>
          </li>
        </ul>
