              <div class="col-lg-4 col-md-6 equel-grid">
                <div class="grid">
                  <div class="grid-body">
                    <p class="card-title">course</p>
                    <div id="radial-chart1"></div>
                    <h4 class="text-center">Digital Marketing</h4>
                    <p class="text-center text-muted">B12 February 2022</p>
                  </div>
                </div>
              </div>
              <div class="col-lg-4 col-md-6 equel-grid">
                <div class="grid">
                  <div class="grid-body">
                    <p class="card-title">course</p>
                    <div id="radial-chart2"></div>
                    <h4 class="text-center">Digital Literacy</h4>
                    <p class="text-center text-muted">B21 January 2022</p>
                  </div>
                </div>
              </div>
              <div class="col-lg-4 col-md-6 equel-grid">
                <div class="grid">
                  <div class="grid-body">
                    <p class="card-title">course</p>
                    <div id="radial-chart3"></div>
                    <h4 class="text-center">UI/UX Design</h4>
                    <p class="text-center text-muted">B13 March 2022</p>
                  </div>
                </div>
              </div>
              <div class="col-md-8 equel-grid">
                <div class="grid">
                  <div class="grid-body py-3">
                    <p class="card-title ml-n1">My course</p>
                  </div>
                  <div class="table-responsive table-outer">
                    <table class="table table-hover table-sm">
                      <thead>
                        <tr class="solid-header">
                          <th colspan="2" class="pl-4">Name</th>
                          <th>Registration No.</th>
                          <th>Registered On</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% appCourses.forEach(appCourse => { %>
                          <tr>
                            <td class="pr-0 pl-4">
                              <img class="profile-img img-sm" src="../assets/images/profile/male/image_4.png" alt="profile image">
                            </td>
                            <td class="pl-md-0">
                              <small class="text-black font-weight-medium d-block"><%= appCourse.courseName %></small>
                              <span class="text-gray">
                                <span class="status-indicator rounded-indicator small <% if(appCourse.approved) { %>bg-success<% } else { %>bg-warning<% } %>"></span><% if(appCourse.approved) { %>Active<% } else { %>Pending<% } %></span>
                            </td>
                            <td>
                              <small><%= appCourse.courseRegNo %></small>
                            </td>
                            <td><%let options = {weekday: 'long', year: 'numeric', month: 'short', day: 'numeric'} %><%= appCourse.createdAt.toLocaleString('en-US', options) %></td>
                          </tr>
                        <% }) %>
                        
                        
                      </tbody>
                    </table>
                  </div>
                  <a class="border-top px-3 py-2 d-block text-gray" href="mycourse?q=<%= userId %>">
                    <small class="font-weight-medium"><i class="mdi mdi-chevron-down mr-2"></i>View All Course</small>
                  </a>
                </div>
              </div>
              <div class="col-md-4 equel-grid">
                <div class="grid">
                  <div class="grid-body">
                    <div class="split-header">
                      <p class="card-title">Activity Log</p>
                    </div>
                    <div class="vertical-timeline-wrapper">
                      <div class="timeline-vertical dashboard-timeline">

                        <% activities.forEach(activity => { %>
                          <div class="activity-log">
                            <p class="log-name"><%= students.name %></p>
                            <div class="log-details"><%= activity.message %></div>
                            <small class="log-time"><%let options = { year: '2-digit', month: '2-digit', day: '2-digit', hour: '2-digit', minute: '2-digit'} %><%= activity.createdAt.toLocaleString('en-US', options) %></small>
                          </div>
                        <% }) %>
                      
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
